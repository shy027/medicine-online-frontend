{
  "name": "medicine-online-frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "axios": "^1.7.8",
    "core-js": "^3.37.1",
    "element-plus": "^2.9.0",
    "roboto-fontface": "*",
    "swiper": "^11.1.15",
    "vue": "^3.4.31",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.14.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "npm-run-all2": "^7.0.1",
    "sass": "1.77.8",
    "sass-embedded": "^1.77.8",
    "typescript": "~5.6.3",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "unplugin-vue-router": "^0.10.0",
    "vite": "^5.4.10",
    "vite-plugin-vuetify": "^2.0.3",
    "vite-plugin-windicss": "^1.9.3",
    "vue-router": "^4.4.0",
    "vue-tsc": "^2.1.10",
    "windicss": "^3.5.6"
  },
  "lint-staged": {
    "src/**/*.{js,cjs,ts,vue}": [
      "npm run lint:fix"
    ],
    "src/**/*.{html,json,css,scss}": [
      "npx prettier --write"
    ]
  }
}
